{% extends 'YusukeSnapBundle::base.html.twig' %}
{% block content %}
    {{ knp_pagination_render(Models) }}
    {% for Model in Models %}
        {% if Model.pic1 is defined %}
            <a href="{{ path('model', {'modelId': Model.id}) }}"><img src="{{ Model.pic1 }}"></a>
        {% endif %}
        {{ Model.updatedAt|date('Y-m-d') }}

    {% endfor %}
    {{ knp_pagination_render(Models) }}
{% endblock %}